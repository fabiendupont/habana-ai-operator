apiVersion: apps/v1
kind: Deployment
metadata:
  name: controller-manager
  namespace: system
spec:
  template:
    spec:
      containers:
        - name: manager
          env:
            - name: "DCGM_EXPORTER_IMAGE"
              value: {{ DCGM_EXPORTER_IMAGE }}
            - name: "DEVICE_PLUGIN_IMAGE"
              value: {{ DEVICE_PLUGIN_IMAGE }}
